    "version": "0.2.0",
    "configurations": [


       {
         "type": "node",
         "request": "launch",
         "name": "Debug Express Server",
         "program": "${workspaceFolder}/server/index.js",
         "runtimeExecutable": "npm",
         "runtimeArgs": ["run", "dev"],
         "restart": true,
         "console": "integratedTerminal",
         "env": {
           "NODE_ENV": "development"
         },
         "skipFiles": ["<node_internals>/**"]
       }
    ]
}